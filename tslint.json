{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rules": {
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "trace"]
        },
        "max-line-length": [true, 150],
        "semicolon": {
            "options": [
                "always"
            ]
        }
    }, 
    "jsRules": {
        "no-console": "off"
    }
} 